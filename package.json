{
  "name": "esti",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "dev": "vite",
    "dev:arc": "BROWSER='Arc' vite --open",
    "db": "firebase emulators:start --import=./database --only firestore",
    "esti": "pnpm db & pnpm dev:arc",
    "lint": "eslint .",
    "types": "vue-tsc --noEmit",
    "build": "vite build",
    "generate": "vite-ssg build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:prod": "MY_BASE_URL=https://esti.patbird.me/ pnpm exec playwright test"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.22",
    "@vueuse/core": "10.11.0",
    "@vueuse/head": "2.0.0",
    "firebase": "10.12.4",
    "floating-vue": "5.2.2",
    "pinia": "2.2.0",
    "vue": "3.4.34",
    "vue-router": "4.4.0",
    "vuefire": "3.1.24"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.24.0",
    "@iconify/json": "2.2.232",
    "@playwright/test": "1.45.3",
    "@types/node": "20.14.13",
    "@unocss/eslint-config": "0.61.8",
    "@unocss/preset-attributify": "0.61.8",
    "@unocss/preset-icons": "0.61.8",
    "@unocss/reset": "0.61.8",
    "@unocss/transformer-variant-group": "0.61.8",
    "@vitejs/plugin-vue": "5.1.1",
    "critters": "0.0.24",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.27.0",
    "firebase-admin": "12.3.0",
    "happy-dom": "13.10.1",
    "typescript": "5.5.4",
    "unocss": "0.61.8",
    "unplugin-auto-import": "0.18.2",
    "unplugin-icons": "0.19.1",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.3.5",
    "vite-plugin-pages": "0.32.3",
    "vite-plugin-vue-devtools": "7.3.7",
    "vite-plugin-vue-layouts": "0.11.0",
    "vite-ssg": "0.23.8",
    "vitest": "1.6.0",
    "vue-tsc": "1.8.27"
  }
}
