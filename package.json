{
  "name": "esti",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.2",
  "scripts": {
    "dev": "vite",
    "dev:arc": "BROWSER='Arc' vite --open",
    "db": "firebase emulators:start --import=./database --only firestore",
    "esti": "pnpm db & pnpm dev:arc",
    "lint": "eslint .",
    "types": "vue-tsc --noEmit",
    "build": "vite build",
    "generate": "vite-ssg build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:prod": "MY_BASE_URL=https://esti.patbird.me/ pnpm exec playwright test"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.23",
    "@vueuse/core": "12.0.0",
    "@vueuse/head": "2.0.0",
    "firebase": "^10.1.0",
    "floating-vue": "5.2.2",
    "pinia": "2.3.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vuefire": "3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.12.0",
    "@iconify/json": "2.2.285",
    "@playwright/test": "1.49.1",
    "@types/node": "22.10.2",
    "@unocss/eslint-config": "0.65.1",
    "@unocss/preset-attributify": "0.65.1",
    "@unocss/preset-icons": "0.65.1",
    "@unocss/reset": "0.65.1",
    "@unocss/transformer-variant-group": "0.65.1",
    "@vitejs/plugin-vue": "5.2.1",
    "beasties": "^0.2.0",
    "eslint": "9.17.0",
    "eslint-plugin-vue": "9.32.0",
    "firebase-admin": "^12.0.0",
    "happy-dom": "15.11.7",
    "typescript": "5.7.2",
    "unocss": "0.65.1",
    "unplugin-auto-import": "0.19.0",
    "unplugin-icons": "0.22.0",
    "unplugin-vue-components": "0.28.0",
    "vite": "6.0.3",
    "vite-plugin-pages": "0.32.4",
    "vite-plugin-vue-devtools": "7.6.8",
    "vite-plugin-vue-layouts": "0.11.0",
    "vite-ssg": "0.24.3",
    "vitest": "2.1.8",
    "vue-tsc": "2.1.10"
  }
}
